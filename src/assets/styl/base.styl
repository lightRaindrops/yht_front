.content-body
  figure.image
    text-align: center;

    figcaption
      text-align: center;
      font-size: 12px;


.shadow
  box-shadow: 0 1px 1px rgba(26, 26, 26, .3);
.el-select .el-input
  //width: 100px;
.history-wappler
  .filter__program
    .el-date-editor--date
      width: 100%;
